knitr::opts_chunk$set(echo = TRUE)
install.packages("caret")
data(mtcars)
View(mtcars)
?mtcars
library(caret)
# modelo de regressão linear simples
model <- train(mpg ~ wt,
data = mtcars,
method = "lm")
# modelo de regressão linear múltipla
model <- train(mpg ~ .,
data = mtcars,
method = "lm")
# modelo utilizando regressão ridge
model <- train(mpg ~ .,
data = mtcars,
method = "ridge") # pode ser 'lasso'
